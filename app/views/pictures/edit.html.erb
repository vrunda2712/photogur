<h1>Edit Picture</h1>
<p>
  <%= link_to("Back", "/pictures/#{@pictures.id}") %>
</p>

<hr>
<%= form_for(@pictures, url: "/pictures/#{@pictures.id}") do |f| %>
  <p>
    <%= f.label :artist %>
    <%= f.text_field :artist %>
  </p>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :url %>
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
